datasource db {
  provider = "sqlite"

  // this could also be env("DATABASE_URL")
  url = "file:database.db"
}

generator client {
  provider  = "prisma-client-py"
  interface = "sync"
}

model Url {
  id       Int      @id @default(autoincrement())
  created  DateTime @default(now())
  original String
  clicks   Int      @default(0)
}